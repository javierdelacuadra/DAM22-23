<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane fx:id="root" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.pantallas.mainscreen.MainController">
    <top>
        <MenuBar>
            <Menu fx:id="optionsMenu" mnemonicParsing="false" text="Options">
                <MenuItem fx:id="salirMenuItem" mnemonicParsing="false" onAction="#salir" text="Exit"/>
            </Menu>
            <Menu fx:id="camionesMenu" mnemonicParsing="false" text="Camiones" onAction="#listCamionesMenu"/>
            <Menu fx:id="conductoresMenu" mnemonicParsing="false" text="Conductores" onAction="#listConductoresMenu"/>
            <Menu fx:id="empresasMenu" mnemonicParsing="false" text="Empresas" onAction="#listEmpresasMenu"/>
        </MenuBar>
    </top>
</BorderPane>
