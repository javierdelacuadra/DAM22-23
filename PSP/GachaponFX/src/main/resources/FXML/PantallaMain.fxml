                <MenuItem fx:id="logoutAdmin" mnemonicParsing="false" text="Cerrar sesión"/>
                <MenuItem fx:id="salirAdmin" mnemonicParsing="false" onAction="#salir" text="Salir"/>
            </Menu>
        </MenuBar>
    </top>
    <Label layoutX="333.0" layoutY="36.0" prefHeight="46.0" prefWidth="139.0" text="Gachapón">
        <font>
